{
  "name": "GetSet Local Storage",
  "version": "0.1.0",
  "description": "Get All the Local Storage values of one domain and set all those values in other domain's local storage",
  "permissions": ["scripting", "activeTab", "storage"],
  "action": {
    "default_title": "Save Local Storage",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/favicon16x16.png",
    "32": "images/favicon32x32.png",
    "64": "images/favicon64x64.png"
  },
  "manifest_version": 3
}